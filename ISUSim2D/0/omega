FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 21.9;

boundaryField
{

	atmosphere
	{
		type			zeroGradient;
	}
    "(ground|walls)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }
	symPlane
    {
        type            symmetryPlane;
    }
}


// ************************************************************************* //
